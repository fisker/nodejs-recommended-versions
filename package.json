{
  "name": "nodejs-recommended-versions",
  "version": "4.1.0",
  "description": "Node.js versions you should install",
  "homepage": "https://github.com/fisker/nodejs-recommended-versions#readme",
  "bugs": {
    "url": "https://github.com/fisker/nodejs-recommended-versions/issues"
  },
  "repository": "fisker/nodejs-recommended-versions",
  "funding": "https://github.com/fisker/nodejs-recommended-versions?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "module": "./index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "build:js": "echo no need",
    "clean": "run-p \"clean:*\"",
    "clean:dist": "del-cli dist",
    "dist": "run-p \"dist:*\"",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p \"format:*\"",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "markdownlint-cli2-fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "eslint \"**/*.{js,jsx,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint-cli2",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier . --check",
    "prepare": "husky install",
    "release": "run-s clean lint test dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "all-node-versions": "11.0.1"
  },
  "devDependencies": {
    "@fisker/eslint-config": "10.1.5",
    "@fisker/eslint-config-ava": "2.0.5",
    "@fisker/husky-config": "4.1.2",
    "@fisker/lint-staged-config": "3.1.5",
    "@fisker/markdownlint-cli2-config": "0.0.3",
    "@fisker/prettier-config": "5.0.6",
    "ava": "4.3.1",
    "del-cli": "5.0.0",
    "eslint": "8.22.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "markdownlint-cli2": "0.5.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "sort-package-json": "1.57.0"
  },
  "packageManager": "yarn@3.2.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
